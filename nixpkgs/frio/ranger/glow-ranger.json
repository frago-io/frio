{
  "document": {
    "block_prefix": "\n",
    "block_suffix": "\n",
    "margin": 2
  },
  "block_quote": {
    "indent": 1,
    "indent_token": "| "
  },
  "paragraph": {},
  "list": {
    "level_indent": 2
  },
  "heading": {
    "block_suffix": "\n",
    "color": "6",
    "bold": true
  },
  "h1": {
    "prefix": " ",
    "suffix": " ",
    "color": "3",
    "bold": true
  },
  "h2": {
    "prefix": "## ",
    "color": "6",
    "bold": true
  },
  "h3": {
    "prefix": "### ",
    "color": "6",
    "bold": true
  },
  "h4": {
    "prefix": "#### ",
    "color": "6"
  },
  "h5": {
    "prefix": "##### ",
    "color": "6"
  },
  "h6": {
    "prefix": "###### ",
    "color": "6"
  },
  "text": {},
  "strikethrough": {
    "crossed_out": true
  },
  "emph": {
    "color": "3"
  },
  "strong": {
    "bold": true
  },
  "hr": {
    "format": "\n--------\n"
  },
  "item": {
    "block_prefix": "* "
  },
  "enumeration": {
    "block_prefix": ". "
  },
  "task": {
    "ticked": "[x] ",
    "unticked": "[ ] "
  },
  "link": {
    "color": "4",
    "underline": true
  },
  "link_text": {
    "color": "5",
    "bold": true
  },
  "image": {
    "color": "5",
    "underline": true
  },
  "image_text": {
    "format": "Image: {{.text}}"
  },
  "code": {
    "color": "1"
  },
  "code_block": {
    "color": "2",
    "margin": 2
  },
  "table": {
    "color": "7"
  },
  "definition_list": {},
  "definition_term": {},
  "definition_description": {
    "block_prefix": "\n> "
  },
  "html_block": {},
  "html_span": {}
}
